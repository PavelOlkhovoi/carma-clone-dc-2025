{
  "name": "geo-helpers",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libraries/commons/geo/helpers/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": [
        "{options.outputPath}"
      ],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/libraries/commons/geo/helpers"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": [
        "{options.reportsDirectory}"
      ],
      "options": {
        "reportsDirectory": "../../../../coverage/libraries/commons/geo/helpers"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
