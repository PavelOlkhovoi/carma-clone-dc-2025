{
  "name": "geo-utils",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libraries/commons/geo/utils/src",
  "projectType": "library",
  "tags": ["type:util", "scope:geo"],
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libraries/commons/geo/utils"
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{workspaceRoot}/coverage/libraries/commons/geo/utils"],
      "options": {
        "reportsDirectory": "../../../../coverage/libraries/commons/geo/utils"
      }
    }
  }
}
