{
  "name": "cesium-api",
  "$schema": "../../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libraries/mapping/engines/cesium/api/src",
  "projectType": "library",
  "tags": ["type:library", "scope:cesium"],
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": [
        "{options.outputPath}"
      ],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/libraries/mapping/engines/cesium/api"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": [
        "{options.reportsDirectory}"
      ],
      "options": {
        "reportsDirectory": "../../../../../coverage/libraries/mapping/engines/cesium/api"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
